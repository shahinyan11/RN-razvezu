{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@httpClient": [
        "./src/httpClient"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@constants/*": [
        "./src/constants/*"
      ],
      "@navigation/*": [
        "./src/navigation/*"
      ],
      "@screens/*": [
        "./src/screens/*"
      ],
      "@store": [
        "./src/store"
      ],
      "@store/*": [
        "./src/store/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@validations/*": [
        "./src/validations/*"
      ],
      "@hooks/*": [
        "./src/hooks/*"
      ],
      "@helpers/*": [
        "./src/helpers/*"
      ],
      "@services/*": [
        "./src/services/*"
      ]
    }
  }
}
